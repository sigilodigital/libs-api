{
  "name": "libs-api",
  "version": "1.10.0",
  "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"200\" alt=\"Nest Logo\" /></a> </p>",
  "main": "index.js",
  "scripts": {
    "git:clu": "git config --local user.name \"Haroldo Cruz\" ; git config --local user.email \"haroldocruzdr@gmail.com\"",
    "git:status:short": "git status -sbv",
    "git:diff": "git diff",
    "git:diff:cached": "git diff --cached",
    "git:diff:nolock": "git diff -- ':!*lock.json'",
    "git:diff:explain:br": "npm run git:diff:nolock | npm run explain:br",
    "git:diff:explain:en": "npm run git:diff:nolock | npm run explain:en",
    "commit": "npm run git:status:short > $npm_package_config_git_commit_message ; npm run git:diff:nolock >> $npm_package_config_git_commit_message ; cat $npm_package_config_git_commit_message | npm run explain:br",
    "commit:message:br": "npm run git:status:short > $npm_package_config_git_commit_message ; npm run git:diff | npm run explain:br >> $npm_package_config_git_commit_message",
    "explain:en": "$npm_package_config_cody $(cat $npm_package_config_prompt) 'en-us' >> $npm_package_config_git_commit_message",
    "explain:br": "$npm_package_config_cody $(cat $npm_package_config_prompt) 'pt-br' > $npm_package_config_git_commit_message",
    "find:pack:update": "npx ncu",
    "circular:madge": "npx madge --circular --extensions ts ./",
    "circular:dpdm": "npx dpdm ./common/src/common.module.ts ./audit/src/audit.module.ts",
    "sshkey": "ssh-keygen -t ed25519 -C "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigilodigital/libs-api.git"
  },
  "author": "Haroldo Cruz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sigilodigital/libs-api/issues"
  },
  "homepage": "https://github.com/sigilodigital/libs-api#readme",
  "dependencies": {
    "@nestjs/axios": "^3.0.3",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^10.4.4",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^16.4.5",
    "jwt-decode": "^4.0.0",
    "pg": "^8.13.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.1",
    "@sourcegraph/cody": "^5.5.15",
    "@types/bcrypt": "^5.0.2",
    "dpdm": "^3.14.0",
    "madge": "^8.0.0",
    "npm-check-updates": "^17.1.3",
    "supertest": "^7.0.0",
    "typeorm-model-generator": "^0.4.6"
  },
  "config":{
    "prompt": "./.misc/prompt",
    "cody": "npx cody chat --stdin -m",
    "git_commit_message": "./.misc/last_commit_message.md"
  }
}
